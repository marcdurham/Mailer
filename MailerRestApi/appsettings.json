{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "EmailSenders": [
    {
      "Name": "SMTP",
      "Enabled": false
    },
    {
      "Name": "SendGrid",
      "Enabled": false
    },
    {
      "Name": "SaveToFile",
      "Enabled": true
    }
  ],
  "DryMode": true,
  "ForceSend": false,
  "TimerIntervalSeconds": 900
}
